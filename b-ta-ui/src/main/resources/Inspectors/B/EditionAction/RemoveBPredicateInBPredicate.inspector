				data="data.subPredicate"
				constraints="twocols(expandHorizontally=true;expandVertically=false;index=3;location=right)"
				visible="">
				<Assignment variable="component.bindable" value="data" />
			</Custom>
		</Tab>
	</TabPanel>
	<FIBVariable name="data"
		type="org.openflexo.ta.b.fml.editionaction.RemoveBPredicateInBPredicate" />
