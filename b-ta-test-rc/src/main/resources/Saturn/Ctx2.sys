SYSTEM
    Ctx2
SEES
    Ctx0
SETS
    MI ;
    MO ;
    TT
CONCRETE_CONSTANTS
    agents_in ,
    agents_out ,
    VIN ,
    VOUT ,
    vec_to_in ,
    vec_to_out ,
    VBF
PROPERTIES
    agents_in <: MI &
    agents_out <: MO &
    VIN = agents_in >-> TT &
    VOUT = agents_out >-> TT &
    vec_to_in : VIN >-> T_IN &
    vec_to_out : VOUT >-> T_OUT &
    VBF : VIN --> VOUT &
    VBF = ( vec_to_in ; FB ; vec_to_out ~ ) &
    ran ( vec_to_out ) <: ran ( FB )
END
